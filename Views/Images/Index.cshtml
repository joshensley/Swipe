@model Swipe.ViewModels.ProfileViewModel
@{
    ViewData["Title"] = "Images";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="text-center mt-3">
    <h1 class="display-4">@Model.ApplicationUser.FirstName's Pictures</h1>
</div>

<div class="pb-2 mt-3">
    <div class="row d-flex justify-content-center">
        @foreach (var item in Model.FirebaseImagesURL)
        {
            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 mt-2">
                <img src="@item" alt="profile" class="img-fluid img-thumbnail" />
            </div>
        }
    </div>
</div>
